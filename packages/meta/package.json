{
  "private": false,
  "name": "@projectit/meta",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "bin": {
    "projectit": "dist/bin/projectit-generate-editor.js"
  },
  "scripts": {
    "start": "echo Starting meta",
    "build": "tsc && yarn run copy-scss1 && yarn run copy-scss2",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "copy-scss1": "cp ./src/styles/*.scss ./dist/styles",
    "copy-scss2": "cp ./src/editor/*.scss ./dist/editor",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873"
  },
  "devDependencies": {
    "@types/pegjs": "^0.10.1",
    "parjs": "0.12.0",
    "pegjs": "^0.10.0",
    "ts-pegjs": "^0.2.6"
  },
  "dependencies": {
    "@microsoft/ts-command-line": "4.3.10",
    "@projectit/core": "^0.0.5",
    "@projectit/model": "^0.0.5",
    "axios": "0.18.0",
    "global": "^4.4.0",
    "parjs": "0.12.0",
    "prettier": "^1.15.3",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
