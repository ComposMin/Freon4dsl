/* This file contains the default editor definition. */

editor default

Test {[
    Name: ${self.name}
    Topic: ${self.topic} using fules from ${flow}

    ${self.scenarios}
]}

Scenario {[
    Scenario: ${self.description}

    ${self.steps}
]}

Step {[
    On page ${self.fromPage} answers are ${self.answersGiven vertical} to page ${self.toPage}

]}

ExitStep {[
    Exit subject
]}

//Nothing {[
//    No Answer
//]}

Answer {[
    ${question} = ${expr}
]}
