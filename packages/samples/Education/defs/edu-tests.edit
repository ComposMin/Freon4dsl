/* This file contains the default editor definition. */

editor default

Test {[
    Name: ${self.name}
    Topic: ${self.topic}

    ${self.scenarios}
]}

Scenario {[
    Description: ${self.description}

    ${self.steps}
]}

Step {[
    On page ${self.fromPage} answers are ${self.answersGiven vertical} to page ${self.toPage}

]}

ExitStep {[
    Exit subject
]}

Nothing {[
    No Answer
]}
