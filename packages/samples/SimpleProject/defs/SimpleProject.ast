language SimpleProject

model SimpleModel {
    parts: SimplePart[];
}

modelunit SimplePart {
    basis: ConceptA;
}

concept ConceptA {
    name: string;
    partA1: ConceptB;
    partA2: ConceptE;
}

concept ConceptB {
    list1: ConceptD[];
    partB2: ConceptE;
}

concept ConceptD {
    street: string;
    nr: number;
}

concept ConceptE {
    leaf: string;
}
