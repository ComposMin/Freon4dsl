editor myTables

Method {
    [
        method ${name} ( ${parameters table rows} ): ${declaredType} {
            ${body}
        }
    ]
}

Parameter {
    table [
        Type            | Name
        ${declaredType} | ${name}
    ]
}
Attribute {
    table [
        ${name} | ${declaredType}
    ]
}

