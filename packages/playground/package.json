{
  "private": true,
  "name": "@projectit/playground",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "scripts": {
    "projectIt": 	"ts-node ../meta/src/commandline/projectit -v generate-all -d src/demo/defs -o src/demo",
    "projectIt-nonVerbose": 	"ts-node ../meta/src/commandline/projectit generate-all -d src/demo/defs -o src/demo",
    "test-expressions": 		"ts-node ../meta/src/commandline/projectit -v test-expressions -l src/demo/defs/DemoDefinition.lang -X src/demo/defs/LanguageExpressions.pitest -o src/demo",
    "metaIt": 		"ts-node ../meta/src/commandline/projectit -v generate-language -l src/demo/defs/DemoDefinition.lang -o src/demo",
    "scopeIt": 		"ts-node ../meta/src/commandline/projectit -v generate-scoper -l src/demo/defs/DemoDefinition.lang -s src/demo/defs/DemoDefinition.scop -o src/demo",
    "editIt": 		"ts-node ../meta/src/commandline/projectit -v generate-editor -l src/demo/defs/DemoDefinition.lang -e src/demo/defs/DemoDefinition.edit -o src/demo",   
    "validateIt": 	"ts-node ../meta/src/commandline/projectit -v generate-validator -l src/demo/defs/DemoDefinition.lang -c src/demo/defs/LanguageDefinition.valid -o src/demo",
    "typeIt": 	"ts-node ../meta/src/commandline/projectit -v generate-typer -l src/demo/defs/DemoDefinition.lang -t src/demo/defs/LanguageDefinition.type -o src/demo",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build-wait": "tsc -w",
    "build": "yarn run projectIt-nonVerbose && tsc ",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest"
  },
  "devDependencies": {
  },
  "dependencies": {
    "@projectit/core": "^0.0.5",
    "@projectit/meta": "^0.0.5",
    "@types/lodash": "4.14.73",
    "@types/pegjs": "^0.10.1",
    "global": "^4.4.0",
    "prettier": "^1.15.3",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "private"
  }
}
