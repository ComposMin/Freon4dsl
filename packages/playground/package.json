{
  "private": true,
  "name": "@projectit/playground",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "scripts": {
    "projectit": 	"ts-node ../meta/src/commandline/projectit -v all -d src/demo/defs -o src/demo",
    "projectit-watch": 	"ts-node ../meta/src/commandline/projectit -vw all -d src/demo/defs -o src/demo",
    "projectit-nonverbose": 	"ts-node ../meta/src/commandline/projectit all -d src/demo/defs -o src/demo",
    "test-expressions": 		"ts-node ../meta/src/commandline/projectit -v test-expressions -l src/demo/defs/LanguageDefinition.lang -X src/demo/defs/LanguageExpressions.pitest -o src/demo",
    "meta-it": 		"ts-node ../meta/src/commandline/projectit -v meta-it -l src/demo/defs/LanguageDefinition.lang -o src/demo",
    "scope-it": 		"ts-node ../meta/src/commandline/projectit -v scope-it -l src/demo/defs/LanguageDefinition.lang -s src/demo/defs/LanguageDefinition.scop -o src/demo",
    "edit-it": 		"ts-node ../meta/src/commandline/projectit -v edit-it -l src/demo/defs/LanguageDefinition.lang -e src/demo/defs/LanguageDefinition.edit -o src/demo",   
    "validate-it": 	"ts-node ../meta/src/commandline/projectit -v validate-it -l src/demo/defs/LanguageDefinition.lang -c src/demo/defs/LanguageDefinition.valid -o src/demo",
    "taxrules": 	"ts-node ../meta/src/commandline/projectit all -d src/taxrules/defs -o src/taxrules",
    "testproject": "ts-node ../meta/src/commandline/projectit -v meta-it -l handmade/Testproject.lang -o src/aa-testproject",
    "metameta": 	"ts-node ../meta/src/commandline/projectit -v all -d src/aa-testproject/defs -o src/aa-testproject && tsc",
    "json": 	"ts-node ../meta/src/commandline/projectit all -d src/json/defs -o src/json",
    "type-it": 	"ts-node ../meta/src/commandline/projectit -v type-it -l src/demo/defs/LanguageDefinition.lang -t src/demo/defs/LanguageDefinition.type -o src/demo",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build-wait": "tsc -w",
    "build": "yarn run projectit-nonverbose && yarn run taxrules && tsc ",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest",
    "calculator": "ts-node ../meta/src/commandline/projectit -v all -d src/calculator/defs -o src/calculator",
    "expressions": "ts-node ../meta/src/commandline/projectit -v all -d src/expressions/defs -o src/expressions"
  },
  "devDependencies": {
    "@uifabric/example-data": "^7.0.9",
    "@uifabric/fluent-theme": "^7.1.51",
    "@fluentui/react": "^7.105.13",
    "@fluentui/react-northstar": "^0.47.0",
    "@types/pegjs": "^0.10.1",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/react-router": "^5.1.4",
    "@types/webpack": "^4.39.2",
    "@types/webpack-env": "^1.14.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-plugin": "^3.2.0",
    "mobx-devtools": "^0.9.21",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@projectit/core": "^0.0.5",
    "@projectit/meta": "^0.0.5",
    "@types/lodash": "4.14.73",
    "@types/history": "^4.7.5",
    "global": "^4.4.0",
    "mobx": "^5.14.0",
    "mobx-react": "6.1.3"
  },
  "publishConfig": {
    "access": "private"
  }
}
