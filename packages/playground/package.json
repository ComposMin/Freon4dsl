{
  "private": true,
  "name": "@projectit/playground",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "scripts": {
    "generate-language": "ts-node ../meta/src/commandline/projectit -l src/LanguageDefinition.lang -o \"src\" generate-language",
    "build-wait": "tsc -w",
    "build": "tsc && yarn run copy-scss1",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "copy-scss1": "cp ./src/styles/*.scss ./dist/styles",
    "copy-scss2": "cp ./src/editor/*.scss ./dist/editor",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873"
  },
  "devDependencies": {
  },
  "dependencies": {
    "@projectit/core": "^0.0.5",
    "@projectit/model": "^0.0.5",
    "global": "^4.4.0",
    "prettier": "^1.15.3",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "private"
  }
}
