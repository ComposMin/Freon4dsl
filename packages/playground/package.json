{
  "private": true,
  "name": "@projectit/playground",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "scripts": {
    "metaIt": "ts-node ../meta/src/commandline/projectit -l src/demo/defs/LanguageDefinition.lang -o src/demo generate-language",
    "scopeIt": "ts-node ../meta/src/commandline/projectit -l src/demo/defs/LanguageDefinition.lang -o src/demo generate-scoper -s src/demo/defs/LanguageDefinition.scop",
    "editIt": "ts-node ../meta/src/commandline/projectit -l src/demo/defs/LanguageDefinition.lang -o src/demo generate-editor -e src/demo/defs/LanguageDefinition.edit",
    "validateIt": "ts-node ../meta/src/commandline/projectit -l src/demo/defs/LanguageDefinition.lang -o src/demo generate-validator -v src/demo/defs/LanguageDefinition.valid",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build-wait": "tsc -w",
    "build": "yarn run metaIt && yarn run scopeIt && yarn run editIt && yarn run validateIt && tsc",
    "lint": "tslint -c ./tslint.json './src/**/*.ts*'",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest"
  },
  "devDependencies": {
  },
  "dependencies": {
    "@projectit/core": "^0.0.5",
    "@projectit/meta": "^0.0.5",
    "@types/lodash": "4.14.73",
    "@types/pegjs": "^0.10.1",
    "global": "^4.4.0",
    "prettier": "^1.15.3",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "private"
  }
}
