editor default

referenceSeparator [::]

XXunit {
    [
    UnitA ${name}
    [?concepts ${namedConcepts horizontal separator [,]} ;]
    ${lines terminator[;]}
    ]
}

ExpWithType {
    [${expr} : ${type}]
}

// EXPRESSIONS
NumberLiteral {
    [${xx}]
}

StringLiteral {
    [${xx}]
}

BooleanLiteral {
    [${xx}]
}

NamedExp {
    [(${inner} as ${myType})]
}

PlusExp {
    [${left} + ${right}]
}

UnitLiteral {
    // 62 kilogram, or 112 miles
    [${inner} ${unit}]
}

GenericLiteral {
    // Set{ 12, 14, 16, 18 }
    [${kind}{ ${content horizontal separator[,]} }]
}

// TYPES
NamedType {
    [${name}]
}

GenericType {
    [${kind}< ${baseType} >]
}

UnitOfMeasurement {
    [${unit}< ${baseType} >]
}
