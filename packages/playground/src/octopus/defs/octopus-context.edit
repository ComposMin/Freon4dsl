editor YYY for language octopus

OclPart {
    @projection normal
        [OCL expressions for ${self.name}
            ${self.constraints @vertical @separator[\n]}
        ]
}

OclContext {
    @projection normal
    [
    context ${self.owningModelelement}
    inv: ${self.expression}
    ]

}

