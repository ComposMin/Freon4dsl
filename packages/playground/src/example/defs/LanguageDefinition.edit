AbsExpression {
    @projection default [
        abs( ${self.expr} )
    ]
}

PlusExpression {
    @symbol "+"
}

MultiplyExpression {
    @symbol "*"
}

DivideExpression {
    @symbol "/"
}
LessThenExpression {
    @symbol "<"
}
OrExpression {
    @symbol "or"
}



Entity {
    @trigger "entity"
}

