BooleanLiteralExpression {
    @projection [ ${self.value} ]
}

MethodCallExpression {
    @projection MethodCallExpression
        [
        CALL ${self.methodDefinition} (  )
        ]
    @trigger "method"
}


Model {
    @projection Model
        [
        model ${self.name} {
        entities:
            ${self.entities
                @vertical
                @terminator [\n\n]
            }
        model wide Methods:
            ${self.methods @horizontal @separator [\n] }
        }
        ]
}
