editor XXX for language ExpressionLibrary

OrExpression {
    @symbol "or"
}
AndExpression {
    @symbol "and"
}
LessThenExpression {
    @symbol "<"
}
LessEqExpression {
    @symbol "<="
}
GreaterThenExpression {
    @symbol ">"
}
GreaterEqExpression {
    @symbol ">="
}
EqualsExpression {
    @symbol "=="
}
NotEqualsExpression {
    @symbol "!="
}
ImpliesExpression {
    @symbol "==>"
}
NotExpression {
    @projection [not ${self.inner}]
    @symbol "not"
}
