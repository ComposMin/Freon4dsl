{
  "private": true,
  "name": "@projectit/test",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "type": "module",
  "scripts": {
    "cleanup":      "bash ../../scripts/cleanup.sh",
    "meta-it": 		"bash ../../scripts/projectit-dev.sh -v meta-it -l src/demo/defs/LanguageDefinition.ast -o src/demo",
    "scope-it":     "bash ../../scripts/projectit-dev.sh -v scope-it -l src/demo/defs/LanguageDefinition.ast -s src/demo/defs/LanguageDefinition.scope -o src/demo",
    "edit-it": 		"bash ../../scripts/projectit-dev.sh -v edit-it -l src/demo/defs/LanguageDefinition.ast -e src/demo/defs/LanguageDefinition.edit -o src/demo",
    "validate-it": 	"bash ../../scripts/projectit-dev.sh -v validate-it -l src/demo/defs/LanguageDefinition.ast -c src/demo/defs/LanguageDefinition.valid -o src/demo",
    "type-it": 	    "bash ../../scripts/projectit-dev.sh -v type-it -l src/demo/defs/LanguageDefinition.ast -t src/demo/defs/LanguageDefinition.type -o src/demo",
    "demo": 	    "bash ../../scripts/projectit-dev.sh all -d src/demo/defs -o src/demo",
    "octopus":      "bash ../../scripts/projectit-dev.sh all -d src/octopus-small/defs -o src/octopus-small",
    "parser-conc":  "bash ../../scripts/projectit-dev.sh all -d src/parser-basic-concepts/defs -o src/parser-basic-concepts",
    "parser-props": "bash ../../scripts/projectit-dev.sh all -d src/parser-basic-properties/defs -o src/parser-basic-properties",
    "parsergen":    "bash ../../scripts/projectit-dev.sh all -d src/parser_gen/defs -o src/parser_gen",
    "imports":      "bash ../../scripts/projectit-dev.sh all -d src/testCircularImports/def -o src/testCircularImports",
    "defaultscoper":"bash ../../scripts/projectit-dev.sh all -d src/testDefaultScoper/defs -o src/testDefaultScoper",
    "definedscoper":"cp src/testDefaultScoper/defs/Language.ast src/testDefinedScoper/defs/Language.ast && cp src/testDefaultScoper/__tests__/SimpleModelCreator.ts src/testDefinedScoper/__tests__/ModelCreator.ts && bash ../../scripts/projectit-dev.sh all -d src/testDefinedScoper/defs -o src/testDefinedScoper",
    "langconstr":   "bash ../../scripts/projectit-dev.sh all -d src/testLangConstructs/defs -o src/testLangConstructs",
    "noparser":     "bash ../../scripts/projectit-dev.sh all -d src/testNoParserAvailable/defs -o src/testNoParserAvailable",
    "testproject":  "bash ../../scripts/projectit-dev.sh all -d src/testproject/defs -o src/testproject",
    "vehicles":     "bash ../../scripts/projectit-dev.sh all -d src/vehicles/defs -o src/vehicles",
    "build":        "yarn demo && yarn octopus && yarn parser-conc && yarn parser-props && yarn parsergen && yarn imports && yarn defaultscoper && yarn definedscoper && yarn langconstr && yarn noparser && yarn testproject && yarn vehicles && tsc",
    "clean-config": "rm */*/projectit/ProjectItConfiguration.ts",
    "clean-custom": "rm */*/*/Custom*.ts",
    "clean-parser": "rm -r */*/*parse*",
    "clean":        "yarn clean-config && yarn clean-custom",
    "clean-demo": 	      "bash ../../scripts/projectit-dev.sh clean-it -d src/demo/defs -o src/demo",
    "clean-octopus":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/octopus-small/defs -o src/octopus-small",
    "clean-parser-conc":  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/parser-basic-concepts/defs -o src/parser-basic-concepts",
    "clean-parser-props": "bash ../../scripts/projectit-dev.sh clean-it -f -d src/parser-basic-properties/defs -o src/parser-basic-properties",
    "clean-parsergen":    "bash ../../scripts/projectit-dev.sh clean-it -f -d src/parser_gen/defs -o src/parser_gen",
    "clean-imports":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/testCircularImports/def -o src/testCircularImports",
    "clean-defaultscoper":"bash ../../scripts/projectit-dev.sh clean-it -f -d src/testDefaultScoper/defs -o src/testDefaultScoper",
    "clean-definedscoper":"bash ../../scripts/projectit-dev.sh clean-it -f -d src/testDefinedScoper/defs -o src/testDefinedScoper",
    "clean-langconstr":   "bash ../../scripts/projectit-dev.sh clean-it -f -d src/testLangConstructs/defs -o src/testLangConstructs",
    "clean-noparser":     "bash ../../scripts/projectit-dev.sh clean-it -f -d src/testNoParserAvailable/defs -o src/testNoParserAvailable",
    "clean-testproject" : "bash ../../scripts/projectit-dev.sh clean-it -f -d src/testproject/defs -o src/testproject",
    "clean-vehicles":     "bash ../../scripts/projectit-dev.sh clean-it -f -d src/vehicles/defs -o src/vehicles",
    "clean-all-projects": "yarn clean-demo && yarn clean-octopus && yarn clean-parser-conc && yarn clean-parser-props && yarn clean-parsergen && yarn clean-imports && yarn clean-defaultscoper && yarn clean-definedscoper && yarn clean-langconstr && yarn clean-noparser && yarn clean-testproject && yarn clean-vehicles",
    "test":         "jest"
  },
  "dependencies": {
    "@projectit/core": "0.2.0",
    "@projectit/meta": "0.2.0",
    "@types/history": "^4.7.5",
    "net.akehurst.language-agl-processor": "4.0.0-rc.5",
    "kotlin": "^1.5.21",
    "global": "^4.4.0",
    "mobx": "6.3.3",
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "access": "private"
  }
}
