UmlClass {
    @projection normal
        [${self.visibility} <class> ${self.name}
        [?<attributes> ${self.attributes @vertical @terminator [\n]}]
        [?<operations> ${self.operations @vertical @terminator [\n] }]
        <endclass>
        ]
}

