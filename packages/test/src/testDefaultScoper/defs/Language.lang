language DefaultScoperTest

model DSmodel {
    name: string;
    units: DSunit[];
}

public modelunit DSunit {
    public name: string;
    public dsParts: DSpart[];
    public reference dsRefs: DSref[];
    dsPrivates: DSprivate[];
}

public concept DSpart {
    public name: string;
    public conceptParts: DSpart[];
    public reference conceptRefs: DSref[];
    conceptPrivates: DSprivate[];
}

public concept DSref {
    name: string;
    conceptParts: DSpart[];
    conceptPrivates: DSprivate[];
    reference conceptRefs: DSref[];
}

concept DSprivate {
    name: string;
    conceptParts: DSpart[];
    conceptPrivates: DSprivate[];
    reference conceptRefs: DSref[];
}

