// for testing primitives
PrimExtra {
    @projection normal [
        before ${self.primIdentifierWithExtra} after
        before ${self.primNumberWithExtra} after
        before ${self.primStringWithExtra} after
        before ${self.primBooleanWithExtra} after
        before ${self.primListIdentifierWithExtra} after
        before ${self.primListNumberWithExtra} after
        before ${self.primListStringWithExtra} after
        before ${self.primListBooleanWithExtra}    after
    ]
}

PrimExtraOptional {
    @projection normal [PrimExtraOptional
        [?before ${self.primIdentifierWithExtra} after]
        [?before ${self.primNumberWithExtra} after]
        [?before ${self.primStringWithExtra} after]
        [?before ${self.primBooleanWithExtra} after]
        [?before ${self.primListIdentifierWithExtra} after]
        [?before ${self.primListNumberWithExtra} after]
        [?before ${self.primListStringWithExtra} after]
        [?before ${self.primListBooleanWithExtra} after]
    ]
}

PrimOptionalSeparator {
    @projection normal [PrimOptionalSeparator
        [?before ${self.primListIdentifierSeparator @separator [, ]} after]
        [?before ${self.primListNumberSeparator @separator [, ]} after]
        [?before ${self.primListStringSeparator @separator [, ]} after]
        [?before ${self.primListBooleanSeparator @separator [, ]} after]
    ]
}

PrimOptionalTerminator {
    @projection normal [PrimOptionalTerminator
        [?before ${self.primListIdentifierTerminator @terminator [! ]} after]
        [?before ${self.primListNumberTerminator @terminator [! ]} after]
        [?before ${self.primListStringTerminator @terminator [! ]} after]
        [?before ${self.primListBooleanTerminator @terminator [! ]} after]
    ]
}

PrimWith {
    @projection normal [ PrimWithKeywordProj [?${self.primBoolean @keyword [<BOOL>]}] ]
}
// for limited concepts
VisibilityKind {
    @projection normal [
        ${PUBLIC @keyword [+]}
        ${PRIVATE @keyword [-]}
        ${PROTECTED @keyword [#]}
    ]
}

LimitedNonOptional {
    @projection normal [
    LimitedNonOptional
    ${self.visibility} ;
    ${self.visMulti @horizontal @separator[, ]} ;
    before ${self.visWithExtra} after ;
    before ${self.visMultiWithExtra  @horizontal @separator[, ]} after ;
    ${self.other} ;
    ${self.otherMulti @horizontal @separator[, ]} ;
    ]
}

LimitedOptional {
    @projection normal [
    LimitedOptional
    [?${self.visibility} ;]
    [?${self.visMulti @horizontal @separator[, ]} ;]
    [?before ${self.visWithExtra} after ;]
    [?before ${self.visMultiWithExtra  @horizontal @separator[, ]} after ;]
    ]
}

// for part properties
WithDirectParts { 
    @projection normal [
    WithDirectParts part ${self.part}
	[?optPart ${self.optPart}]
	partList ${self.partList}
	[?optList ${self.optList} ]
	]
}
